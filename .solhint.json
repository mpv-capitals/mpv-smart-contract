{
  "extends": "default",
  "rules": {
    "indent": ["error", 4],

    "bracket-align": false,
    "compiler-fixed": false,
    "no-simple-event-func-name": false,
    "two-lines-top-level-separator": false,
    "code-complexity": false,
    "function-max-lines": false,
    "not-rely-on-time": false
  }
}
